<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/m05_log_activity_fragment"
    android:background="@color/colorWhiteSmoke"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fitucab.ds1617b.fitucab.UI.Activities.M05AddExerciseActivity">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/tv_m05_titulotipo"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:layout_x="31dp"
        android:layout_y="30dp"
        android:fontFamily="sans-serif"
        android:typeface="sans"

        android:text="@string/_tv_m05_type"
        android:textSize="20dp"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="31dp" />

    <TextView
        android:id="@+id/tv_m05_startime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:typeface="sans"
        android:layout_x="35dp"
        android:layout_y="153dp"

        android:text="@string/_tv_m05_startime"
        android:textSize="20dp"
        android:layout_marginTop="69dp"
        android:layout_below="@+id/tv_m05_titulotipo"
        android:layout_alignLeft="@+id/tv_m05_titulotipo"
        android:layout_alignStart="@+id/tv_m05_titulotipo" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_m05_duration"
        android:layout_alignStart="@+id/tv_m05_duration"
        android:layout_below="@+id/tv_m05_duration"
        android:layout_marginTop="61dp"
        android:layout_x="38dp"
        android:layout_y="341dp"
        android:fontFamily="sans-serif"
        android:text="@string/_tv_m05_calories"
        android:textSize="20dp"
        android:typeface="sans" />

    <TextView
        android:id="@+id/tv_m05_duration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_m05_startime"
        android:layout_alignStart="@+id/tv_m05_startime"
        android:layout_below="@+id/tv_m05_startime"
        android:layout_marginTop="55dp"
        android:layout_x="39dp"
        android:layout_y="256dp"
        android:fontFamily="sans-serif"
        android:text="@string/_tv_m05_duration"
        android:textSize="20dp"
        android:typeface="sans" />

    <EditText
        android:id="@+id/et_m05_editcalories"
        android:layout_width="272dp"
        android:layout_height="wrap_content"
        android:enabled="true"
        android:focusable="true"
        android:layout_x="74dp"
        android:layout_y="371dp" />

    <Spinner
        android:id="@+id/spn_m05_types"
        android:layout_width="242dp"
        android:layout_height="41dp"
        android:layout_alignLeft="@+id/tv_m05_startime"
        android:layout_alignStart="@+id/tv_m05_startime"
        android:layout_below="@+id/tv_m05_titulotipo"
        android:layout_marginTop="23dp"
        android:layout_x="64dp"
        android:layout_y="79dp"
        android:background="@color/common_google_signin_btn_text_dark_disabled"
        android:enabled="true"
        android:focusable="true"
        android:fontFamily="sans-serif"
        android:typeface="sans" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="108dp"
        android:layout_height="54dp"
        android:layout_x="233dp"
        android:layout_y="191dp"
        android:focusable="true"
        android:enabled="true"
        android:ems="10"
        android:inputType="time" />

    <EditText
        android:id="@+id/editText3"

        android:layout_width="111dp"
        android:layout_height="50dp"
        android:focusable="true"
        android:layout_x="75dp"
        android:enabled="true"
        android:layout_y="194dp"
        android:ems="10"
        android:inputType="date" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="40dp"
        android:layout_height="37dp"
        android:layout_x="30dp"
        android:layout_y="299dp"
        app:srcCompat="@mipmap/ic_launcher_time" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="42dp"
        android:layout_height="37dp"
        android:layout_x="194dp"
        android:layout_y="202dp"
        app:srcCompat="@mipmap/ic_launcher_time" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="39dp"
        android:layout_height="40dp"
        android:layout_x="31dp"
        android:layout_y="202dp"
        app:srcCompat="@mipmap/ic_launcher_date" />

    <EditText
        android:id="@+id/et_m05_editduration"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:layout_x="70dp"
        android:inputType="number"
        android:layout_y="297dp"
        android:enabled="true"
        android:focusable="true" />

    <TextView
        android:id="@+id/tv_manual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_m05_duration"
        android:layout_alignStart="@+id/tv_m05_duration"
        android:layout_below="@+id/tv_m05_duration"
        android:layout_marginTop="61dp"
        android:layout_x="40dp"
        android:layout_y="565dp"
        android:fontFamily="sans-serif"
        android:text="@string/_tv_m05_titletabmanual"
        android:textColor="@color/colorwhite"
        android:textSize="20dp"
        android:typeface="sans" />

    <Button
        android:id="@+id/btn_m05_manualregister"
        android:layout_width="188dp"
        android:layout_height="63dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="41dp"
        android:layout_x="106dp"
        android:layout_y="456dp"
        android:width="200dp"
        android:background="@color/colorGreenBtn"
        android:backgroundTint="@color/colorGreenBtn"
        android:fontFamily="sans-serif"
        android:gravity="center"
        android:text="@string/_btn_m05_registermanual"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
        android:textColor="@color/colorwhite"
        android:textSize="20dp"
        android:textStyle="bold"
        android:typeface="sans" />

</AbsoluteLayout>
